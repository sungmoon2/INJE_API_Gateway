# Server Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Fabric Configuration
FABRIC_CONNECTION_PROFILE=./config/connection-profile.json
FABRIC_CHANNEL_NAME=your-channel-name
FABRIC_CHAINCODE_NAME=your-chaincode
FABRIC_CONTRACT_NAME=YourContract
FABRIC_USER_ID=appUser
FABRIC_MSP_ID=YourMSP

# Fabric CA Configuration
FABRIC_CA_URL=http://localhost:7054
FABRIC_CA_CONFIG=./config/ca-config.json
FABRIC_CA_ADMIN_ID=admin
FABRIC_CA_ADMIN_SECRET=your-admin-password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# Security
API_KEYS=your-api-key-1,your-api-key-2,your-api-key-3
WEBHOOK_SECRET=your-webhook-secret
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:4000,https://your-domain.com

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100